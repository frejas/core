{
    "name": "frejas/core",
    "description": "Core of Frejas.",
    "type": "project",
    "require": {
        "hrafn/router": "0.5.0",
        "jitesoft/container": "4.0.*",
        "jitesoft/loggers": "2.1.*",
        "jitesoft/exceptions": "2.3.*",
        "guzzlehttp/psr7": "1.9.*",
        "ext-json": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.*",
        "phpunit/phpunit": "9.1.*",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Frejas\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frejas\\Core\\Tests\\": "tests/"
        }
    }
}
